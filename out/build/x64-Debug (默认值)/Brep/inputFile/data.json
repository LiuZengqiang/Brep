{
  "Face": {
    // 需要被sweep的面的轮廓坐标，该例子中是一个正方形
    "Vertex": [
      [ 0.5, 0.5 ],
      [ -0.5, 0.5 ],
      [ -0.5, -0.5 ],
      [ 0.5, -0.5 ]
    ],
    // Face面的内环，该样例中有两个内环，两个内环一个正方形一个是三角形
    "Ring": [
      [
        // 第一个内环，矩形内环
        [ 0.475, 0.475 ],
        [ 0.125, 0.475 ],
        [ 0.125, 0.125 ],
        [ 0.475, 0.125 ]
      ],
      [
        // 第二个内环，三角形内环
        [ -0.125, -0.125 ],
        [ -0.475, -0.125 ],
        [ -0.475, -0.475 ]
      ]
    ]
  },
  // 扫成方向
  "SweepVector": [
    0,
    0,
    -1
  ],
  // 扫成长度
  "SweepScale": 0.2,
  // 显示时面的颜色
  "Color": [
    1.0,
    0.0,
    0.0
  ]
}